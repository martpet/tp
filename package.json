{
  "name": "tp",
  "version": "0.0.19",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start": "vite serve frontend",
    "all": "npm run build && npm test && npm run check && npm run lint && npm run synth",
    "test": "npm run test:frontend -- -w=false && npm run test:backend -- -w=false && npm run test:shared -- -w=false",
    "test:frontend": "vitest --config frontend/vitest.config.ts --segfault-retry=3",
    "test:backend": "vitest --config backend/vitest.config.ts --segfault-retry=3",
    "test:shared": "vitest --config shared/vitest.config.ts --segfault-retry=3",
    "check": "check:frontend && check:backend && check:shared",
    "check:frontend": "tsc --project frontend/tsconfig.json",
    "check:backend": "tsc --project backend/tsconfig.json",
    "check:shared": "tsc --project shared/tsconfig.json",
    "lint": "eslint frontend && eslint backend && eslint shared",
    "synth": "cd backend && npx cdk synth --context envName=production -q",
    "preview": "npm run build && vite preview frontend",
    "build": "npm run intl:extract-compile && vite build frontend",
    "deploy": "npm run check && npm run build && cd backend && NODE_ENV=development npx cdk deploy --all --require-approval=never --context envName=personal",
    "generate-id": "node -e 'import(\"nanoid\").then(({nanoid}) => console.log(nanoid(10)))'",
    "intl:extract": "formatjs extract 'frontend/src/**/*.ts*' --flatten --ignore='**/*.d.ts' --out-file frontend/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile": "formatjs compile-folder --ast frontend/lang frontend/public/compiled-lang",
    "intl:extract-compile": "npm run intl:extract & npm run intl:compile",
    "prepare": "husky install"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.22.0",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.46.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.46.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.46.0-alpha.0",
    "@aws-sdk/client-cloudwatch": "^3.188.0",
    "@aws-sdk/client-dynamodb": "^3.188.0",
    "@aws-sdk/client-sns": "^3.188.0",
    "@aws-sdk/client-ssm": "^3.188.0",
    "@aws-sdk/client-sts": "^3.188.0",
    "@aws-sdk/lib-dynamodb": "^3.188.0",
    "@aws-sdk/util-dynamodb": "^3.188.0",
    "@formatjs/intl": "^2.5.0",
    "@mrgrain/cdk-esbuild": "^3.11.2",
    "@reduxjs/toolkit": "^1.8.6",
    "@vitest/coverage-istanbul": "^0.24.3",
    "app-root-path": "^3.1.0",
    "aws-cdk-lib": "^2.46.0",
    "babel-plugin-formatjs": "^10.3.30",
    "base64url": "^3.0.1",
    "boring-avatars": "^1.7.0",
    "camelcase-keys": "^8.0.2",
    "clone": "^2.1.2",
    "constructs": "^10.1.131",
    "cookie": "^0.5.0",
    "deepmerge": "^4.2.2",
    "dotenv-flow": "^3.2.0",
    "http-status-codes": "^2.2.0",
    "milliseconds": "^1.0.3",
    "module": "^1.2.5",
    "node-fetch": "^3.2.10",
    "npm": "^8.19.2",
    "pkce-challenge": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intl": "^6.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.188.0",
    "@formatjs/cli": "^5.1.2",
    "@types/app-root-path": "^1.2.4",
    "@types/aws-lambda": "^8.10.107",
    "@types/clone": "^2.1.1",
    "@types/cookie": "^0.5.1",
    "@types/dotenv-flow": "^3.2.0",
    "@types/milliseconds": "^0.0.30",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "add": "^2.0.6",
    "aws-cdk": "^2.46.0",
    "aws-sdk-client-mock": "^2.0.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^4.3.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "nanoid": "^4.0.0",
    "prettier": "^2.6.2",
    "redux-logger": "^3.0.6",
    "stylelint": "^14.14.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "type-fest": "^3.1.0",
    "typescript": "4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^2.2.2",
    "vitest": "^0.24.3",
    "yarn": "^1.22.19"
  }
}
