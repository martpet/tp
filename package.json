{
  "name": "tp",
  "version": "0.0.1",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start": "vite serve frontend",
    "all": "npm run check && npm run lint && npm test && npm run build && npm run synth",
    "test": "npm run test:backend -- -w=false && npm run test:shared -- -w=false",
    "test:backend": "vitest --config backend/vitest.config.ts --segfault-retry=3",
    "test:shared": "vitest --config shared/vitest.config.ts --segfault-retry=3",
    "check": "tsc --project frontend/tsconfig.json && tsc --project backend/tsconfig.json && tsc --project shared/tsconfig.json",
    "lint": "eslint frontend && eslint backend && eslint shared",
    "build": "vite build frontend",
    "synth": "cd backend && npx cdk synth --context envName=production -q",
    "deploy": "npm run check && npm run build && cd backend && NODE_ENV=development npx cdk deploy --context envName=personal",
    "random-id": "node -e 'import(\"nanoid\").then(({nanoid}) => console.log(nanoid(10)))'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.40.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.40.0-alpha.0",
    "@aws-sdk/client-cloudwatch": "^3.163.0",
    "@aws-sdk/client-dynamodb": "^3.163.0",
    "@aws-sdk/client-sns": "^3.163.0",
    "@aws-sdk/client-ssm": "^3.163.0",
    "@aws-sdk/client-sts": "^3.163.0",
    "@aws-sdk/lib-dynamodb": "^3.163.0",
    "@aws-sdk/util-dynamodb": "^3.163.0",
    "@vitest/coverage-istanbul": "^0.23.1",
    "app-root-path": "^3.1.0",
    "aws-cdk-lib": "^2.40.0",
    "base64url": "^3.0.1",
    "camelcase-keys": "^8.0.2",
    "constructs": "^10.1.94",
    "cookie": "^0.5.0",
    "deepmerge": "^4.2.2",
    "dotenv-flow": "^3.2.0",
    "http-status-codes": "^2.2.0",
    "module": "^1.2.5",
    "node-fetch": "^3.2.10",
    "pkce-challenge": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.162.0",
    "@types/app-root-path": "^1.2.4",
    "@types/aws-lambda": "^8.10.102",
    "@types/cookie": "^0.5.1",
    "@types/dotenv-flow": "^3.2.0",
    "@types/node": "^18.7.5",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.1.0",
    "add": "^2.0.6",
    "aws-cdk": "^2.40.0",
    "aws-sdk-client-mock": "^2.0.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.6",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "nanoid": "^4.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.11.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "type-fest": "^2.19.0",
    "typescript": "4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^2.1.0",
    "vitest": "^0.23.1",
    "yarn": "^1.22.19"
  }
}
