{
  "name": "tp",
  "version": "0.0.37",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start": "vite serve frontend",
    "all": "npm run build && npm test && npm run check && npm run lint && npm run synth",
    "test": "npm run test:frontend -- -w=false && npm run test:backend -- -w=false && npm run test:shared -- -w=false",
    "test:frontend": "vitest --config frontend/vitest.config.ts --segfault-retry=3",
    "test:backend": "vitest --config backend/vitest.config.ts --segfault-retry=3",
    "test:shared": "vitest --config shared/vitest.config.ts --segfault-retry=3",
    "check": "npm run check:frontend && npm run check:backend && npm run check:shared",
    "check:frontend": "tsc --project frontend/tsconfig.json",
    "check:backend": "tsc --project backend/tsconfig.json",
    "check:shared": "tsc --project shared/tsconfig.json",
    "lint": "eslint frontend && eslint backend && eslint shared",
    "synth": "cd backend && npx cdk synth --context envName=production -q",
    "preview": "npm run build && vite preview frontend",
    "build": "npm run intl:extract-compile && vite build frontend",
    "deploy": "npm run check && npm run build && cd backend && NODE_ENV=development npx cdk deploy --all --require-approval=never --context envName=personal",
    "intl:extract": "formatjs extract 'frontend/src/**/*.ts*' --flatten --ignore='**/*.d.ts' --out-file frontend/src/translations/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile": "formatjs compile-folder --ast frontend/src/translations frontend/public/translations",
    "intl:extract-compile": "npm run intl:extract & npm run intl:compile",
    "prepare": "husky install",
    "random": "node -e 'console.log(crypto.randomBytes(5).toString(`hex`))'"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.23.0",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.51.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.51.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.51.1-alpha.0",
    "@aws-sdk/client-cloudwatch": "^3.215.0",
    "@aws-sdk/client-dynamodb": "^3.215.0",
    "@aws-sdk/client-s3": "^3.215.0",
    "@aws-sdk/client-sns": "^3.215.0",
    "@aws-sdk/client-ssm": "^3.215.0",
    "@aws-sdk/client-sts": "^3.215.0",
    "@aws-sdk/lib-dynamodb": "^3.215.0",
    "@aws-sdk/s3-presigned-post": "^3.215.0",
    "@aws-sdk/util-dynamodb": "^3.222.0",
    "@formatjs/intl": "^2.5.1",
    "@mrgrain/cdk-esbuild": "^3.11.4",
    "@react-spectrum/avatar": "^3.0.0-alpha.11",
    "@react-spectrum/label": "^3.9.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@spectrum-icons/illustrations": "^3.5.1",
    "@vitest/coverage-istanbul": "^0.25.3",
    "app-root-path": "^3.1.0",
    "async-mutex": "^0.4.0",
    "aws-cdk-lib": "^2.51.1",
    "babel-plugin-formatjs": "^10.3.31",
    "base64url": "^3.0.1",
    "boring-avatars": "^1.7.0",
    "camelcase-keys": "^8.0.2",
    "clone": "^2.1.2",
    "constructs": "^10.1.167",
    "cookie": "^0.5.0",
