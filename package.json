    "module": "^1.2.5",
    "node-fetch": "^3.3.0",
    "npm": "^9.1.2",
    "pkce-challenge": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.1",
